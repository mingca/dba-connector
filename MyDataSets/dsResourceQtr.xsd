                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        tblResource.ResourceType, tblResource.ICCResourceID, tblResource.ResourceName, DATEPART(Year, tblResourceRecommend.RecommendDate) AS Yr, DATEPART(q, 
                         tblResourceRecommend.RecommendDate) AS Qtr, tblResourceRecommend.GrantNum, tblOrg.OrgName + '  [' + ISNULL(tblOrg.City, '') + ']' AS OrgandCity, 
                         tblCase.CaseName, tblCase.OpenDate AS CaseOpen, luStatus.StatusName AS CaseStatus, tblResourceRecommend.Used, 
                         tblContact.FirstName + ' ' + tblContact.Lastname AS RecommendedTo, tblCase.CloseDate AS CaseClose, tblResourceFeedback.FeedbackDate, 
                         luCRGTerm.CRGName AS CaseCRG, luStaff.StaffFirstNameFirst AS CaseMgr, tblResourceRecommend.RecommendDate, CASE WHEN recommendstaffnum BETWEEN
                          200 AND 6000 THEN 'Center Staff' ELSE CASE WHEN RecommendedBy &gt; '' THEN 'Congr' ELSE 'unknown' END END AS Discovery, @Start AS ParamStart, 
                         @End AS ParamEnd, @Region AS ParamRegion
FROM            tblGrant RIGHT OUTER JOIN
                         tblOrg RIGHT OUTER JOIN
                         tblResourceRecommend ON tblOrg.OrgID = tblResourceRecommend.OrgNum LEFT OUTER JOIN
                         luStaff RIGHT OUTER JOIN
                         tblCase ON luStaff.StaffID = tblCase.CaseMgrNum ON tblResourceRecommend.CaseNum = tblCase.CaseID LEFT OUTER JOIN
                         luStatus ON tblCase.StatusNum = luStatus.StatusID LEFT OUTER JOIN
                         tblContact ON tblResourceRecommend.ContactNum = tblContact.ContactID LEFT OUTER JOIN
                         tblResourceFeedback ON tblResourceRecommend.RecommendID = tblResourceFeedback.RecommendNum LEFT OUTER JOIN
                         luCRGTerm ON tblCase.CRGNum = luCRGTerm.CRGID ON tblGrant.GrantIDTxt = tblResourceRecommend.GrantNum LEFT OUTER JOIN
                         tblResource ON tblResourceRecommend.ResourceNum = tblResource.ICCResourceID
WHERE        (tblResourceRecommend.RecommendDate BETWEEN @Start AND @End) AND (tblResourceRecommend.OrgNum &lt;&gt; 2300) AND 
                         (tblOrg.SatelliteRegion LIKE @Region) AND (tblResourceRecommend.RecommendedBy IS NULL OR
                         NOT (tblResourceRecommend.RecommendedBy LIKE 'delete%'))
ORDER BY Discovery, tblResource.ResourceName, tblOrg.OrgName, tblCase.CaseName</CommandText>
