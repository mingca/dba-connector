						tblGrantTMGI.GrantAppRecdDt, tblGrantTMGI.TentativePlan, tblGrantTMGI.Notes, tblGrantTMGI.FeePaid, tblGrantTMGI.Hotel, tblGrantTMGI.Stamped,
